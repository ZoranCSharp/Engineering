import java.util.Scanner;

public class Temp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		float temp;
		
		Scanner in = new Scanner(System.in);
		
		System.out.println("Temperatura u Farenhajtu je: ");
		temp = in.nextInt();
		
		temp = ((temp - 32)*5)/9;
		
		System.out.println("Temperatura u Celzijusu je: " +temp);
	}

}
